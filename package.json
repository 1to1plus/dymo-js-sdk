{
  "name": "dymo-js-sdk",
  "version": "3.0.0",
  "homepage": "https://github.com/dymosoftware/dymo-connect-framework",
  "description": "Node project for dymo",
  "private": true,
  "author": {
    "name": "Zach Robichaud",
    "email": "zach2825@gmail.com"
  },
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "clean": "rm -rf dist",
    "debug": "nodemon --exec babel-node --inspect-brk src/index.js ",
    "serve": "node dist/index.js",
    "start": "nodemon src/demo/index.js --exec babel-node",
    "dev": "yarn start",
    "test": "jest --runInBand --verbose",
    "coverage": "jest --coverage --runInBand --verbose",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\"",
    "lint": "eslint src --fix",
    "release": "npm version patch && git push && git push --tags"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "plugins": [
      "node",
      "jest"
    ],
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended",
      "plugin:jest/recommended",
      "prettier"
    ],
    "rules": {},
    "env": {
      "jest/globals": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.4",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "dot-object": "^2.1.4",
    "dotenv": "^16.0.0",
    "fast-xml-parser": "^4.0.3",
    "google-closure-library": "^20220104.0.0",
    "jsonxml": "^1.0.0",
    "lodash": "^4.17.21"
  }
}
